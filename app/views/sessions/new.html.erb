<div>
    <h1>Login</h1>
</div>

<div>

<%= form_for :user do |f| %>

<%= f.label :email %>
<%= f.text_field :email %>

<%= f.label :password %>
<%= f.password_field :password %>

<%= f.submit "Login" %>

<% end %>

</div>
<br>
<p>or click the button below to Login with your Google Account </p>
<%= button_to "Log in with Google" , '/auth/google_oauth2' %>